# Settings related to the Amazon Web Services Route53 DNS service

# The AWS zone identifier
AWS_HOSTED_ZONE_ID="/hostedzone/<%= scope.lookupvar('::openshift_origin::aws_zone_id') %>"

# AWS Access credentials
AWS_ACCESS_KEY_ID="<%= scope.lookupvar('::openshift_origin::aws_access_key_id') %>"
AWS_SECRET_KEY="<%= scope.lookupvar('::openshift_origin::aws_secret_key') %>"
